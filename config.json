{
  "rules": [
    {
      "name": "Images",
      "type": "extension",
      "patterns": [".jpg", ".jpeg", ".png", ".gif", ".bmp", ".svg", ".webp", ".ico"],
      "output_folder": "images"
    },
    {
      "name": "Documents",
      "type": "extension",
      "patterns": [".pdf", ".doc", ".docx", ".txt", ".md", ".xlsx", ".xls", ".pptx", ".ppt", ".csv"],
      "output_folder": "documents"
    },
    {
      "name": "Videos",
      "type": "extension",
      "patterns": [".mp4", ".avi", ".mov", ".mkv", ".wmv", ".flv", ".webm"],
      "output_folder": "videos"
    },
    {
      "name": "Audio",
      "type": "extension",
      "patterns": [".mp3", ".wav", ".flac", ".aac", ".ogg", ".wma", ".m4a"],
      "output_folder": "audio"
    },
    {
      "name": "Archives",
      "type": "extension",
      "patterns": [".zip", ".rar", ".7z", ".tar", ".gz", ".bz2"],
      "output_folder": "archives"
    },
    {
      "name": "Code",
      "type": "extension",
      "patterns": [".py", ".js", ".ts", ".java", ".c", ".cpp", ".cs", ".go", ".rs", ".html", ".css", ".json", ".xml", ".yaml", ".yml", ".sh", ".bat"],
      "output_folder": "code"
    },
    {
      "name": "Temp Files",
      "type": "prefix",
      "patterns": ["tmp_", "temp_", "~"],
      "output_folder": "temp"
    },
    {
      "name": "Backup Files",
      "type": "prefix",
      "patterns": ["bak_", "backup_", "old_"],
      "output_folder": "backup"
    },
    {
      "name": "Report Files",
      "type": "prefix",
      "patterns": ["report_", "rpt_", "summary_"],
      "output_folder": "reports"
    },
    {
      "name": "Log Files",
      "type": "extension",
      "patterns": [".log"],
      "output_folder": "logs"
    }
  ],
  "settings": {
    "unmatched_folder": "others",
    "overwrite": false,
    "dry_run": false
  }
}
